<h1>Create a new Faq</h1>

<%= form_for([@project, @faq]) do |f| %>
  <div class="field">
    <%= f.label :question %>
    <%= f.text_field :question %>
  </div>

  <div class="field">
    <%= f.label :answer %>
    <%= f.text_field :answer %>
  </div>

  <div class="actions">
    <%= f.submit %>
  </div>
<% end %>
